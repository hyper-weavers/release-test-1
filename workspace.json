{
  "version": 2,
  "projects": {
    "workspace": {
      "root": ".",
      "targets": {
        "version": {
          "executor": "@jscutlery/semver:version",
          "options": {
            "syncVersions": true,
            "preset": "conventional",
            "skipProjectChangelog": true,
            "changelogHeader": "# Changelog\n\n"
          }
        }
      }
    },
    "core": "apps/core",
    "core-e2e": "apps/core-e2e"
  }
}
